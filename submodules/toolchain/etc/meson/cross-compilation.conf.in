# Note: Buildroot's and Meson's terminologies differ about the meaning
# of 'build', 'host' and 'target':
# - Buildroot's 'host' is Meson's 'build'
# - Buildroot's 'target' is Meson's 'host'

[binaries]
c = '/opt/armv5-eabi--glibc--stable-2020.02-2/bin/arm-buildroot-linux-gnueabi-gcc'
cpp = '/opt/armv5-eabi--glibc--stable-2020.02-2/bin/arm-buildroot-linux-gnueabi-g++'
ar = '/opt/armv5-eabi--glibc--stable-2020.02-2/bin/arm-buildroot-linux-gnueabi-ar'
strip = '/opt/armv5-eabi--glibc--stable-2020.02-2/bin/arm-buildroot-linux-gnueabi-strip'
pkgconfig = '/opt/armv5-eabi--glibc--stable-2020.02-2/bin/pkgconf'

[properties]
needs_exe_wrapper = true
c_args = ["-D_LARGEFILE_SOURCE", "-D_LARGEFILE64_SOURCE", "-D_FILE_OFFSET_BITS=64", "-Os"@PKG_TARGET_CFLAGS@]
c_link_args = [@PKG_TARGET_CFLAGS@]
cpp_args = ["-D_LARGEFILE_SOURCE", "-D_LARGEFILE64_SOURCE", "-D_FILE_OFFSET_BITS=64", "-Os"@PKG_TARGET_CFLAGS@]
cpp_link_args = [@PKG_TARGET_CFLAGS@]
sys_root = '/opt/armv5-eabi--glibc--stable-2020.02-2/arm-buildroot-linux-gnueabi/sysroot'
pkg_config_libdir = '/opt/armv5-eabi--glibc--stable-2020.02-2/arm-buildroot-linux-gnueabi/sysroot/usr/lib/pkgconfig:/opt/armv5-eabi--glibc--stable-2020.02-2/arm-buildroot-linux-gnueabi/sysroot/usr/share/pkgconfig'
pkg_config_static = '$(if $(BR2_STATIC_LIBS),true,false)'

[host_machine]
system = 'linux'
cpu_family ='arm'
cpu = 'arm926ej-s'
endian = 'little'
